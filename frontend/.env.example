# Study Eyes Frontend Environment Configuration

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:5000/api
REACT_APP_WEBSOCKET_URL=ws://localhost:5001

# Application Configuration
REACT_APP_NAME=Study Eyes
REACT_APP_VERSION=1.0.0
REACT_APP_DESCRIPTION=AI-powered study session monitoring

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_EXPORT=true
REACT_APP_ENABLE_CAMERA=true

# Camera Configuration
REACT_APP_DEFAULT_CAMERA_FACING=user
REACT_APP_CAMERA_QUALITY=high
REACT_APP_CAMERA_RESOLUTION_WIDTH=640
REACT_APP_CAMERA_RESOLUTION_HEIGHT=480

# Session Configuration
REACT_APP_MAX_SESSION_DURATION=7200000  # 2 hours in milliseconds
REACT_APP_SESSION_WARNING_TIME=300000   # 5 minutes warning in milliseconds

# Analytics Configuration
REACT_APP_ANALYTICS_UPDATE_INTERVAL=30000  # 30 seconds in milliseconds
REACT_APP_CHARTS_ANIMATION_DURATION=1000   # 1 second in milliseconds

# UI Configuration
REACT_APP_THEME=dark
REACT_APP_SIDEBAR_COLLAPSED=false

# Development Configuration
REACT_APP_DEBUG=true
REACT_APP_LOG_LEVEL=info

# Notification Configuration
REACT_APP_NOTIFICATION_DURATION=5000
REACT_APP_ENABLE_SOUND=true

# Performance Configuration
REACT_APP_LAZY_LOADING=true
REACT_APP_CACHE_ENABLED=true
